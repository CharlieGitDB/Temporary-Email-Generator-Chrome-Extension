{
  "name": "Temporary email generator for Mail Poof",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Allows you to generate a temporary email on the spot!",
  "homepage_url": "https://www.google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Temporary Email Generator",
    "default_popup": "main.html"
  },
  "permissions": ["tabs"]
}